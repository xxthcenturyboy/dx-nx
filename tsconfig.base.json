{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dx/auth": ["libs/auth/src/index.ts"],
      "@dx/config-api": ["libs/api/config/src/index.ts"],
      "@dx/config-mobile": ["libs/mobile/config/src/index.ts"],
      "@dx/config-shared": ["libs/shared/config/src/index.ts"],
      "@dx/config-web": ["libs/web/config/src/index.ts"],
      "@dx/data-access-postgres": [
        "libs/shared/data-access/postgres/src/index.ts"
      ],
      "@dx/data-access-redis": ["libs/shared/data-access/redis/src/index.ts"],
      "@dx/data-access-web-axios": ["libs/web/data-access/axios/src/index.ts"],
      "@dx/devices": ["libs/devices/src/index.ts"],
      "@dx/email": ["libs/email/src/index.ts"],
      "@dx/healthz": ["libs/healthz/src/index.ts"],
      "@dx/logger": ["libs/logger/src/index.ts"],
      "@dx/mail": ["libs/mail/src/index.ts"],
      "@dx/phone": ["libs/phone/src/index.ts"],
      "@dx/plugins-nx": ["libs/plugins-nx/src/index.ts"],
      "@dx/server": ["libs/server/src/index.ts"],
      "@dx/shortlink": ["libs/shortlink/src/index.ts"],
      "@dx/user": ["libs/user/src/index.ts"],
      "@dx/utils": ["libs/utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
