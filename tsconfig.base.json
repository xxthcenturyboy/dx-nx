{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dx/auth": [
        "libs/auth/src/index.ts"
      ],
      "@dx/config": [
        "libs/config/src/index.ts"
      ],
      "@dx/email": [
        "libs/email/src/index.ts"
      ],
      "@dx/healthz": [
        "libs/healthz/src/index.ts"
      ],
      "@dx/logger": [
        "libs/logger/src/index.ts"
      ],
      "@dx/mail": [
        "libs/mail/src/index.ts"
      ],
      "@dx/phone": [
        "libs/phone/src/index.ts"
      ],
      "@dx/plugins-nx": [
        "libs/plugins-nx/src/index.ts"
      ],
      "@dx/postgres": [
        "libs/data/postgres/src/index.ts"
      ],
      "@dx/redis": [
        "libs/data/redis/src/index.ts"
      ],
      "@dx/server": [
        "libs/server/src/index.ts"
      ],
      "@dx/shortlink": [
        "libs/shortlink/src/index.ts"
      ],
      "@dx/user": [
        "libs/user/src/index.ts"
      ],
      "@dx/utils": [
        "libs/utils/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
